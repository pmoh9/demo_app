<% obj.each do |article| %>
<%= link_to article.title, article_path(article) %></td>
<%= truncate(article.description, length: 50) %></td>
<small>Created by: <%= article.user.username if article.user %>, <%= time_ago_in_words(article.created_at) %> ago, last updated: <%= time_ago_in_words(article.updated_at) %> ago</small>
<%= link_to "Edit", edit_article_path(article) %>
<%= link_to "Delete", article_path(article), method: :delete, data: { confirm: "Are you sure you want to delete this article?"} %>
<% end %>
